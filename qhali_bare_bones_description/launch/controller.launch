<launch>

<rosparam file="$(find qhali_bare_bones_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="qhali_bare_bones" args="left_wheel_joint_position_controller right_wheel_joint_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/qhali_bare_bones/joint_states"/>
</node>

</launch>